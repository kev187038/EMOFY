pipeline {
    agent any
    stages {
        stage('mvn-build') {
            steps {
                bat 'mvn clean package'
            }
        }
        stage('copy') {
            steps {
                bat 'move target\\emofy-0.0.1-SNAPSHOT.jar .'
            }
        }
        stage('docker-compose') {
            steps {
                bat 'docker-compose up'
            }
        }

    }
}
